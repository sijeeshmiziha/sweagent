{
  "name": "sweagent",
  "version": "0.0.3",
  "description": "Enterprise-grade domain-specialized AI agents for software planning â€” multi-stage pipelines, sub-agent orchestration, and structured outputs for planning, requirements, DB design, and frontend generation",
  "author": {
    "name": "sijeeshmiziha",
    "email": "sijeeshmonbalan@gmail.com",
    "url": "https://github.com/sijeeshmiziha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sijeeshmiziha/sweagent.git"
  },
  "homepage": "https://github.com/sijeeshmiziha/sweagent",
  "bugs": {
    "url": "https://github.com/sijeeshmiziha/sweagent/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run typecheck && npm run lint && tsup",
    "dev": "tsup --watch",
    "test": "vitest run tests/unit tests/tools tests/core",
    "test:unit": "vitest run tests/unit tests/tools tests/core",
    "test:integration": "vitest run tests/integration",
    "test:all": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "example": "tsx --env-file=.env",
    "example:interactive": "tsx --env-file=.env examples/run.ts",
    "tokens": "tsx .scripts/token-calculator.ts",
    "tokens:verbose": "tsx .scripts/token-calculator.ts --verbose",
    "tokens:json": "tsx .scripts/token-calculator.ts --json",
    "ci": "npm run typecheck && npm run lint && npm run test:unit && npm run build",
    "release:patch": "npm version patch && git push origin main --tags",
    "release:minor": "npm version minor && git push origin main --tags",
    "release:major": "npm version major && git push origin main --tags",
    "prepublishOnly": "npm run ci",
    "prepare": "husky"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "ai",
    "ai-coding",
    "claude-code",
    "codex",
    "cursor",
    "planning",
    "swe",
    "software-engineering",
    "agents",
    "tools",
    "ai-sdk",
    "openai",
    "anthropic",
    "google"
  ],
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.0",
    "@ai-sdk/google": "^3.0.0",
    "@ai-sdk/openai": "^3.0.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "ai": "^6.0.0",
    "handlebars": "^4.7.8",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@inquirer/prompts": "^7.2.0",
    "@types/node": "^25.2.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0",
    "vitest": "^4.0.18"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.40.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
